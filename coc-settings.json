{
    "pyright.testing.provider": "pytest",
    "htmldjango.formatting.provide" : "none",
    "htmldjango.djlint.enableLint": false,
    "htmldjango.completion.enable": false,
    "tailwindCSS.includeLanguages": { "eelixir": "html", "elixir": "html", "eruby": "html", "html.twig": "html", "htmldjango": "html" },
    "languageserver": {
      "golang": {
        "command": "gopls",
        "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
        "filetypes": ["go"]
      }
    }
}
